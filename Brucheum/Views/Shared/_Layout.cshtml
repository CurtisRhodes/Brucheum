<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - The Brucheum</title>
    <link href="~/Styles/default.css" rel="stylesheet" />
    <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
    <script src="~/Scripts/GlobalFunctions.js"></script>
    <script src="~/Scripts/ResizeThreeColumnPage.js"></script>
</head>
<body>
    @Html.Partial("_bHeader")
    <div class="body-content">
        @RenderBody()
    </div>
    @Html.Partial("_bFooter")
    <div id="modalContainer" class="modalVail">
        <div id="modalContent"></div>
    </div>
    @RenderSection("scripts", required: false)
    <script>resizePage();</script>
</body>
</html>
